{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install-server": "cd proxy-server/server && npm install",
    "start": "node proxy-server/server/index.js",
    "install-client": "cd ../ && npm install",
    "client": "cd ../ && npm start",
    "dev": "concurrently -n 'server,client' -c 'red,green'  \"nodemon proxy-server/server/index.js\" \"npm run client\"",
    "build": "cd proxy-server/src && npm run build",
    "heroku-postbuild": "npm run install-server && cd proxy-server/src && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apicache": "^1.6.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-sslify": "^1.2.0",
    "http-proxy-middleware": "^2.0.6",
    "needle": "^3.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
